@model List<AcunMedyaPortfolyo.Models.Tbl_Job>
<section id="resume" class="resume section light-background">
    <div class="container">


       
    </div>
 <div class="col-lg-6" data-aos="fade-up" data-aos-delay="200">
            <h3 class="resume-title">Deneyim</h3>
            @foreach (var item in Model)
            {
                <div class="resume-item">
                    <h4>@item.Title</h4>
                    <h5>
                        @(item.StartYear.HasValue ? item.StartYear.Value.ToString("yyyy-MM-dd") : "N/A") -
                        @(item.EndYear.HasValue ? item.EndYear.Value.ToString("yyyy-MM-dd") : "N/A")
                    </h5>

                    <p><em>@item.CompanyName</em></p>
                    <ul>
                        <li>@item.Description</li>

                    </ul>
                </div>
            }
         

        </div>

    </div>
